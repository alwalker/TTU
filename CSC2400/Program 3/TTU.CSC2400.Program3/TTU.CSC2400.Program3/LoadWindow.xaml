<Window x:Class="TTU.CSC2400.Program3.LoadWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="LoadWindow" Height="146" Width="300" Loaded="Window_Loaded" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="65*" />
            <RowDefinition Height="32*" />
        </Grid.RowDefinitions>
        <Button Content="Ok" Height="23" HorizontalAlignment="Left" Margin="5,5,5,5" Name="btnOK" VerticalAlignment="Top" Width="75" Grid.Row="1" Click="btnOK_Click" TabIndex="1" IsDefault="True" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Right" Margin="5,5,5,5" Name="btnClose" VerticalAlignment="Top" Width="75" Grid.Row="1" Click="btnClose_Click" TabIndex="2" IsCancel="True" />
        <Label Content="File Name:" HorizontalAlignment="Left" Margin="10,10,0,0" Name="lblFileName" VerticalAlignment="Top" />
        <TextBox Height="29" HorizontalAlignment="Left" Margin="82,10,0,0" Name="txtFileName" VerticalAlignment="Top" Width="184" TabIndex="0" />
    </Grid>
</Window>
