<Window x:Class="TTU.CSC2400.Program3.DijkstraWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="DijkstraWindow" Height="152" Width="300" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" Loaded="Window_Loaded">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="65*" />
            <RowDefinition Height="32*" />
        </Grid.RowDefinitions>
        <Button Content="Ok" Height="23" HorizontalAlignment="Left" Margin="5,5,5,5" Name="btnOK" VerticalAlignment="Top" Width="75" Grid.Row="1" Click="btnOK_Click" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Right" Margin="5,5,5,5" Name="btnClose" VerticalAlignment="Top" Width="75" Grid.Row="1" Click="btnClose_Click" />
        <Label Content="Start Node:" HorizontalAlignment="Left" Margin="10,10,0,0" Name="lblStart" VerticalAlignment="Top" />
        <Label Content="Goal Node:" HorizontalAlignment="Left" Margin="10,39,0,0" Name="lblGoal" VerticalAlignment="Top" />
        <ComboBox Height="24" HorizontalAlignment="Left" Margin="86,12,0,0" Name="cboStartNode" VerticalAlignment="Top" Width="76" SelectionChanged="cboStartNode_SelectionChanged" />
        <ComboBox Height="24" HorizontalAlignment="Left" Margin="86,41,0,0" Name="cboGoalNode" VerticalAlignment="Top" Width="76" SelectionChanged="cboGoalNode_SelectionChanged" />
    </Grid>
</Window>
